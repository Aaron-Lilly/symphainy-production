# Test Environment Configuration for Security Infrastructure
# This file contains test configuration for the 5-layer security architecture

# Supabase Configuration (Test Values)
SUPABASE_URL=https://test-project.supabase.co
SUPABASE_ANON_KEY=test_anon_key_here
SUPABASE_SERVICE_KEY=test_service_key_here

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# JWT Configuration
JWT_SECRET_KEY=test_jwt_secret_key_for_development_only
JWT_ALGORITHM=HS256
JWT_EXPIRATION=3600

# Multi-tenant Configuration
MULTI_TENANT_ENABLED=true
DEFAULT_TENANT_TYPE=individual
MAX_TENANTS_PER_USER=1
TENANT_ISOLATION_STRICT=true
RLS_ENABLED=true

# Security Configuration
SECURITY_LEVEL=standard
PASSWORD_MIN_LENGTH=8
PASSWORD_REQUIRE_UPPERCASE=true
PASSWORD_REQUIRE_LOWERCASE=true
PASSWORD_REQUIRE_NUMBERS=true
PASSWORD_REQUIRE_SYMBOLS=true

# Cache Configuration
CACHE_ENABLED=true
TENANT_CACHE_TTL=3600
USER_CONTEXT_CACHE_TTL=1800
SESSION_CACHE_TTL=3600



